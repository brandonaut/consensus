<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.consensus">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ElectionSetupActivity"
            android:label="@string/election_setup_title"/>
        <activity
            android:name=".ElectionLandingActivity"
            android:label="@string/election_title_default"/>
        <activity
            android:name=".PrepareCandidateListActivity"
            android:label="@string/prepare_candidate_list_title"/>
        <activity
            android:name=".ResultsActivity"
            android:label="@string/results_title"/>
        <activity
            android:name=".BallotActivity"
            android:label="@string/ballot_title"/>
    </application>

</manifest>